@model IEnumerable<Test.Models.LabAsign>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-hover text-white table-bordered">
    <tr class="table-primary text-white">
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Complaint.Complnt_content)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Complaint.Status1.State)
