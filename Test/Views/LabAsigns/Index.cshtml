    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Complaint.Complnt_content)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Complaint.Status1.State)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Labor.Lab_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Organization.org_name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.LbAs_id }) |
                @Html.ActionLink("Details", "Details", new { id = item.LbAs_id }) |
